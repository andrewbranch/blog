  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Deploy",
      "program": "${workspaceFolder}/scripts/deploy.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Build",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["build"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Develop",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["develop"]
    }
  ]
}